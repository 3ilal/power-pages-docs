{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/power-pages/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None"      
    },
    "fileMetadata": {
      "ms.author": {
        "**.*": "ndoelman"
      },
      "author": {
        "**.*": "nickdoelman"
      },
      "searchScope": {
        "**.*": [ "Power Apps", "Power Pages" ]
      },
      "ms.service": {
        "**.*": "powerapps"
      }  
    },
    "template": [],
    "dest": "power-pages-docs"
  }
}
